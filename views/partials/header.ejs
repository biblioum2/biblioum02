<div id="Principal">
  <div class="header">
    <!-- First box -->
     <div class="headerstyle">
     <div id="box1"><p>Seleccionar lenguaje:</p></div>
     <div>
      <select id="idioma" onchange="cambiarIdioma()">
      <option value="en" >English</option>
      <option value="es" selected = "selected">Español</option>
      <option value="fr">Français</option>
      </select>
      <div>
          <b>Horario de atención:</b>
      </div>
     </div>
     <div id="Horario">
      <p id="semana">Lunes - Viernes, 09:00am - 19:00pm.</p>
     </div>
     <div>
      
      <div id="Naranja-fragil"><div id="Naranja-fuerte"></div></div>
     </div>
     </div>
  </div>
    <nav>
      <div class="mobile">
        <!-- Caja responsive -->
        <div class="header">
          <!-- Header y nav -->
          <div class="more"><button>More</button></div> <!-- Boton More -->
        </div>
        <div class="links">
          <!-- vinculos -->
          <a href="#">Services</a>
          <a href="#">About</a>
          <a href="#">Contact</a>
          <a href="#">Search</a>
          <a href="#">Sign up</a>
          <% if (authToken) {%>
          <a href="./login">Estas logueado</a>
          <% } else { %>
          <a href="./login">Login</a>
          <% } %>
        </div>
      </div>
      <div class="desktop">
        <!-- Diseño escritorio -->
        <div class="Logo"><img src="img/uman.png" alt="" /></div> <!-- Palabra logo en nav -->
        <div class="primary">
          <!-- Buscador -->
          <input id="search-input" type="text" autocomplete="off" placeholder="Buscar..." /> <!-- Buscador -->
          <button class="search-nav-btn"><i class="fa-solid fa-magnifying-glass"></i></button> <!-- Buscador -->
          <!-- ⬇ CONTENEDOR PARA ALMACENAR LAS SUGERENCIAS ⬇ -->
          <div id="autocomplete-list" class="autocomplete-suggestions"></div>
        </div>
        <div class="secondary full">
          <!-- Links escritorio en nav -->
          <a href="/">Inicio</a>
          <% if (isAdmin) {%>
            <a href="./admin">Admin</a>
            <% } %>
          <% if (authToken) {%>
          <a href="./logout">Cerrar sesion</a>
          <% } else { %>
          <a href="./login">Login</a>
          <% } %>
        </div>
        <div class="secondary mini">
          <!-- Links en movil -->
          <a href="#" class="more">Más</a> <!-- Boton desplegable -->
          <div class="submenu">
            <!-- Submenu -->
            <a href="#">Regístrate</a>
            <a href="./login">Inicia Sesión</a>
          </div>
        </div>
      </div>
    </nav> <!-- Caja nav -->
  </div> <!-- Header y Navegador -->